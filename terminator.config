[global_config]
  enabled_plugins = TerminalShot, LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  focus = system
  title_transmit_fg_color = "#285ea6"
  title_inactive_fg_color = "#5a6f8c"
  title_transmit_bg_color = "#000000"
  title_inactive_bg_color = "#050d17"
[keybindings]
[profiles]

  # USING DarkFruit BY DEFAULT
  [[default]]
    palette = "#000000:#892183:#a06026:#1e747d:#629223:#89214c:#a09526:#e1e1e1:#555753:#ad0798:#ca6408:#078a97:#6cb907:#ad074d:#cab808:#e9e9e9"
    font = DejaVu Sans Mono 10
    background_color = "#181b21"
    use_system_font = False
    foreground_color = "#cccccc"
  [[LightFruit]]
    palette = "#000000:#892183:#a06026:#1e747d:#629223:#89214c:#a09526:#e1e1e1:#555753:#ad0798:#ca6408:#078a97:#6cb907:#ad074d:#cab808:#e9e9e9"
    font = DejaVu Sans Mono 10
    background_color = "#fafafa"
    use_system_font = False
    foreground_color = "#181b21"
  [[DarkFruit]]
    palette = "#000000:#892183:#a06026:#1e747d:#629223:#89214c:#a09526:#e1e1e1:#555753:#ad0798:#ca6408:#078a97:#6cb907:#ad074d:#cab808:#e9e9e9"
    font = DejaVu Sans Mono 10
    background_color = "#181b21"
    use_system_font = False
    foreground_color = "#cccccc"
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = DarkFruit
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
